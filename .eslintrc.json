{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "eslint:recommended",
        "plugin:@angular-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@angular-eslint/eslint-plugin", "prettier"],
    "rules": {
        "prettier/prettier": "error"
    }
}
